                    option(value='Retail') Retail
                    option(value='Food') Restaurant
                    option(value='Hotel') Hotel
                    option(value='Service') Service
                    option(value='Financial') Financial
                #rangeExtremes.notincluded
                    p(style='display:inline-block;margin:0px;width:20%;height:15px;') $0
                    p(style='display:inline-block;margin:0px;float:right;width:45px;height:15px;') $50,000
                input#compAmount(type='range', value=0,min= 0,max=50000, name='amount')
                
                textarea#compDescr(name='description',rows='5',type='text',placeholder='Description')
                
                
                #create_comp_button.create_compbuffer.notincluded
