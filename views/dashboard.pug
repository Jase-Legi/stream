        #newcompany_form
            fieldset#new_form(action='admin/compcreate/')
                h2#compFormtitle.notincluded Company profile
                
                input#compName(type='text',name='compname',placeholder='Company Name')
                p.notincluded(style='margin:10px 0px 0px 5%;width:90%;height:15px;') Industry
                select#compIndustry(name='industry') Industry
                    option(value='')
